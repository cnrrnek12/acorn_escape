<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<typeAliases>
		<typeAlias type="com.acorn.escape.notice.dto.NoticeDto"
			alias="noticeDto"/>
		<typeAlias type="com.acorn.escape.login.dto.LoginDto" 
			alias="loginDto"/>
		<typeAlias type="com.acorn.escape.timetable.dto.TimetableDto" alias="timetableDto"/>
		<typeAlias type="com.acorn.escape.timetable.dto.ResInfoDto" alias="resinfoDto"/>
		<typeAlias type="com.acorn.escape.confirm.dto.ConfirmDto" alias="confirmDto"/>
		<typeAlias type="com.acorn.escape.review.dto.ReviewDto" alias="reviewDto"/>
	</typeAliases>

	<mappers>
		<mapper resource="com/acorn/escape/mybatis/LoginMapper.xml"/>
		<mapper resource="com/acorn/escape/mybatis/NoticeMapper.xml"/>
		<mapper resource="com/acorn/escape/mybatis/TimetableMapper.xml"/>
		<mapper resource="com/acorn/escape/mybatis/ConfirmMapper.xml"/>
		<mapper resource="com/acorn/escape/mybatis/ReviewMapper.xml"/>
	</mappers>		
</configuration>
